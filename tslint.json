{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
        "no-unused-expression": true
    },
    "rules": {
        "trailing-comma": [ false ],
        "quotemark": [
            true,
            "single"
        ],
        "variable-name": {
            "options": ["allow-leading-underscore"]
        },
        "no-console": [false]
    },
    "rulesDirectory": []
}